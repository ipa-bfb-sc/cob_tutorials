<?xml version="1.0"?>
<launch>

	<include file="$(find cob_scenario_example)/launch/scenario.xml" >
	
		<!-- define pcs -->
		<arg name="pc1" value="$(env ROBOT)-pc1"/>
		<arg name="pc2" value="$(env ROBOT)-pc2"/>
		<arg name="pc3" value="$(env ROBOT)-pc3"/>
		
		<!-- ################################################################ -->
		<!-- ### TODO: uncomment the following lines if your are not      ### -->
		<!-- ###       using default robot or default env config packages ### -->
		<!-- ################################################################ -->
		
		<!-- define location for robot parameters: uncomment if you are not using parameters from cob_default_robot_config -->
		<arg name="robot_config" value="as" /> <!--"$(find cob_scenario_example)/config/robots/upload_robot_param.launch"/-->

		<!-- define location for env parameters: uncomment if you are not using parameters from cob_default_env_config -->
		<arg name="env_config" value="$(find cob_scenario_example)/config/envs/upload_env_param.launch"/>

		<!-- define location for map: uncomment if you are not using a map from cob_default_env_config -->
		<arg name="map" value="$(find cob_scenario_example)/config/envs/$(env ROBOT_ENV)/map.yaml"/>

	</include>

</launch>
