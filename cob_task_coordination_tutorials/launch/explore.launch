<?xml version="1.0"?>
<launch>

	<!-- load objects -->
	<rosparam command="load" ns="objects" file="$(find cob_task_coordination_tutorials)/config/$(env ROBOT_ENV)/object_locations.yaml"/>
	<node name="spawn_all_objects" pkg="cob_bringup_sim" type="spawn_object.py" args="all" respawn="false" output="screen" />

	<!-- start navigation -->
	<include file="$(find cob_navigation_global)/launch/2dnav_ros_dwa.launch"/>

	<!-- start perception -->
	<include file="$(find cob_object_detection_fake)/ros/launch/object_detection_fake.launch"/>

</launch>
